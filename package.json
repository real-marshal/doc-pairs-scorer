{
  "name": "doc-pairs-scorer",
  "version": "0.0.1",
  "author": "Real_Marshal",
  "main": "src/index.ts",
  "packageManager": "yarn@3.3.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --transpile-only --cache-directory .cache/ts-node-dev ./src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./dist/src node -r tsconfig-paths/register dist/src/index.js",
    "postinstall": "husky install",
    "lint": "eslint . --cache --cache-location .cache/eslint --cache-strategy content",
    "lint:nocache": "eslint .",
    "format": "prettier --check ./src",
    "commit": "gacp --no-add --emoji emoji",
    "migrate:make": "yarn knex migrate:make",
    "migrate:list": "yarn knex migrate:list",
    "migrate:up": "cross-env NODE_OPTIONS='-r ts-node/register' knex migrate:up",
    "migrate:down": "cross-env NODE_OPTIONS='-r ts-node/register' knex migrate:down",
    "apigen": "openapi -i \"http://$(node -r dotenv-defaults/config -e 'console.log(process.env.HOST)'):$(node -r dotenv-defaults/config -e 'console.log(process.env.PORT)')/docs/json\" -o openapi_typescript_codegen --indent 2  --name API && rollup -c  --bundleConfigAsCjs && rimraf openapi_typescript_codegen"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/better-sqlite3": "^7.6.3",
    "@types/ejs": "^3",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-unicorn": "^45.0.1",
    "gacp": "^3.0.3",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "knex": "^2.3.0",
    "openapi-typescript-codegen": "^0.23.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "rimraf": "^4.1.2",
    "rollup": "^3.12.0",
    "semantic-release": "^19.0.5",
    "semantic-release-docker": "^2.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.8.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/view": "^7.4.1",
    "@sinclair/typebox": "^0.25.13",
    "ajv": "^8.11.2",
    "better-sqlite3": "^8.0.1",
    "dotenv-defaults": "^5.0.2",
    "ejs": "^3.1.8",
    "fastify": "^4.10.2",
    "fastify-plugin": "^4.4.0",
    "lodash": "^4.17.21",
    "tsconfig-paths": "^4.1.1"
  }
}
